<p>
  <strong>Wiki Name:</strong>
  <div>
  <%= @wiki.name %>
  </div>
  <% if current_user && @wiki.administered_by?(current_user) %>
  <div>
  <%= link_to 'Destroy', wiki_path(@wiki),
              method: :delete,
              data: { confirm: 'Are you sure?' } %>
  </div>
  <% end %>
</p>

<%= link_to 'Back', wikis_path %>
