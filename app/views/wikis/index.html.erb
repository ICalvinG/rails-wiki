<h2 class="title-for-line">wikirailsopedia wikis:</h2>

  <ul>
  <% @wikis.each do |wiki| %>
    <div class="wiki-name"><h3><li><%= link_to wiki.name, wiki_path(wiki) %></li></h3></div>

      <% if current_user && wiki.administered_by?(current_user) %>
    <div><%= link_to 'edit this wiki', edit_wiki_path(wiki) %> â€¢ <%= link_to 'delete this wiki', wiki_path(wiki), method: :delete, data: { confirm: 'Are you sure?' } %></div>
    </ul>

  <div class="create-new-wiki-link"><%= link_to "don't see a wiki you like? create a new wiki!", new_wiki_path %></div>


      <% end %>
  <% end %>