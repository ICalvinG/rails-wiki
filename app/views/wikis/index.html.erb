<h1>Available Wikis:</h1>

<table>
  <tr>
    <th>Name</th>
  </tr>



  <% @wikis.each do |wiki| %>
    <tr>
      <td><%= wiki.name %></td>
      <td><%= link_to 'Show', wiki_path(wiki) %></td>
      <td><%= link_to 'Edit', edit_wiki_path(wiki) %></td>
    </tr>
  <% end %>
</table>

<%= link_to 'New Group', new_wiki_path %>

<%= form_tag("/search", method: "get") do %>
  <%= label_tag(:name, "Search for:") %>
  <%= text_field_tag(:name) %>
  <%= submit_tag("Search") %>
<% end %>
