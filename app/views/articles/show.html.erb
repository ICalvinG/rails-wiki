<h4>Title:</h4> <%= @article.title %><br>
<h4>Body:</h4> <%= @snapshot.body.html_safe %><br>
<%= link_to 'Show', article_path(@article) %>


  <% if current_user && @wiki.administered_by?(current_user) %>
    <%= link_to 'Edit', edit_article_path(@article) %>
    <%= link_to 'Destroy', article_path(@article), method: "delete" %><br><br>
<% end %>
Click here to see your <%= link_to 'revision', snapshot_path(@snapshot) %> history!